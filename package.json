{
  "name": "@absaoss/ng-tools-dev-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open --port 4500 --proxy-config proxy.conf.json",
    "build": "ng build --prod",
    "build-lib": "ng build ng-tools --prod && npm run copy-lib-assets && npm run copy-lib-secondary-assets",
    "build-lib-for-publish": "npm run lint && npm run test-lib && npm run build-lib",
    "test": "jest --passWithNoTests --coverage",
    "test:no-covarege": "jest --passWithNoTests",
    "lint": "npm run lint:ts && npm run lint:styles",
    "lint:ts": "eslint \"src/**/*.ts\" \"projects/**/*.ts\" --quiet",
    "lint:ts:fix": "eslint \"src/**/*.ts\" \"projects/**/*.ts\" --quiet --fix",
    "lint:styles": "stylelint \"src/**/*.scss\" \"projects/**/*.scss\"",
    "lint:styles:fix": "stylelint \"src/**/*.scss\" \"projects/**/*.scss\" --fix",
    "pre-pull-request": "npm run lint && npm run test:no-covarege && npm run license-check",
    "ngcc": "ngcc --properties es2015 browser module main --async false",
    "copy-lib-assets": "shx cp -r \"projects/ng-tools/**/assets/\" \"dist/ng-tools/\" && shx cp -r \"projects/ng-tools/**/styles/\" \"dist/ng-tools/\""
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "11.2.13",
    "@angular/cdk": "11.2.12",
    "@angular/common": "11.2.13",
    "@angular/compiler": "11.2.13",
    "@angular/core": "11.2.13",
    "@angular/forms": "11.2.13",
    "@angular/material": "11.2.12",
    "@angular/platform-browser": "11.2.13",
    "@angular/platform-browser-dynamic": "11.2.13",
    "@angular/router": "11.2.13",
    "bootstrap": "5.0.0",
    "hammerjs": "2.0.8",
    "moment": "2.29.1",
    "rxjs": "6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "0.11.3"
  },
  "devDependencies": {
    "@angular-builders/jest": "11.2.0",
    "@angular-devkit/build-angular": "0.1102.12",
    "@angular-eslint/builder": "4.2.1",
    "@angular-eslint/eslint-plugin": "4.2.1",
    "@angular-eslint/eslint-plugin-template": "4.2.1",
    "@angular-eslint/template-parser": "4.2.1",
    "@angular/cli": "11.2.12",
    "@angular/compiler-cli": "11.2.13",
    "@types/jest": "26.0.23",
    "@types/lodash": "4.14.169",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/eslint-plugin-tslint": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "codelyzer": "^6.0.0",
    "eslint": "7.26.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-unused-imports": "1.1.1",
    "husky": "6.0.0",
    "jest": "26.6.3",
    "jest-preset-angular": "8.4.0",
    "lint-staged": "11.0.0",
    "lodash": "4.17.21",
    "ng-packagr": "11.2.4",
    "shx": "0.3.3",
    "stylelint": "13.13.1",
    "stylelint-config-sass-guidelines": "8.0.0",
    "stylelint-scss": "3.19.0",
    "ts-node": "8.3.0",
    "tslint": "6.1.0",
    "tslint-angular": "3.0.3",
    "typescript": "4.1.5"
  }
}
