{
  "name": "@absaoss/ang-tools-dev-app",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open --port 4600",
    "build": "ng build -c production",
    "build-lib": "ng build ang-tools -c production && npm run copy-lib-assets",
    "build-lib-for-publish": "npm run lint && npm run test-lib && npm run build-lib",
    "test": "jest --passWithNoTests --coverage",
    "test-lib": "jest projects/ang-tools --passWithNoTests --coverage",
    "test:no-covarege": "jest --passWithNoTests",
    "lint": "npm run lint:ts && npm run lint:styles",
    "lint:ts": "eslint \"src/**/*.ts\" \"projects/**/*.ts\" --quiet",
    "lint:ts:fix": "eslint \"src/**/*.ts\" \"projects/**/*.ts\" --quiet --fix",
    "lint:styles": "stylelint \"src/**/*.scss\" \"projects/**/*.scss\"",
    "lint:styles:fix": "stylelint \"src/**/*.scss\" \"projects/**/*.scss\" --fix",
    "pre-pull-request": "npm run lint && npm run test:no-covarege && npm run license-check",
    "ngcc": "ngcc --properties es2015 browser module main --async false",
    "copy-lib-assets": "shx cp -r \"projects/ang-tools/**/assets/\" \"dist/ang-tools/\" && shx cp -r \"projects/ang-tools/**/styles/\" \"dist/ang-tools/\""
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "12.0.4",
    "@angular/cdk": "12.0.4",
    "@angular/common": "12.0.4",
    "@angular/compiler": "12.0.4",
    "@angular/core": "12.0.4",
    "@angular/forms": "12.0.4",
    "@angular/material": "12.0.4",
    "@angular/platform-browser": "12.0.4",
    "@angular/platform-browser-dynamic": "12.0.4",
    "@angular/router": "12.0.4",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "bootstrap": "5.0.0",
    "lodash-es": "4.17.21",
    "moment": "2.29.1",
    "ngx-translate-multi-http-loader": "3.0.0",
    "rxjs": "6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "12.1.0",
    "@angular-devkit/build-angular": "12.0.4",
    "@angular-eslint/builder": "4.2.1",
    "@angular-eslint/eslint-plugin": "4.2.1",
    "@angular-eslint/eslint-plugin-template": "4.2.1",
    "@angular-eslint/template-parser": "4.2.1",
    "@angular/cli": "12.0.4",
    "@angular/compiler-cli": "12.0.4",
    "@types/jest": "26.0.23",
    "@types/lodash": "4.14.170",
    "@types/lodash-es": "4.17.4",
    "@types/node": "15.12.2",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/eslint-plugin-tslint": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "codelyzer": "^6.0.0",
    "eslint": "7.26.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-unused-imports": "1.1.1",
    "husky": "6.0.0",
    "jest": "27.0.4",
    "jest-preset-angular": "9.0.4",
    "lint-staged": "11.0.0",
    "lodash": "4.17.21",
    "ng-packagr": "12.0.5",
    "shx": "0.3.3",
    "stylelint": "13.13.1",
    "stylelint-config-sass-guidelines": "8.0.0",
    "stylelint-scss": "3.19.0",
    "ts-jest": "27.0.3",
    "ts-node": "8.3.0",
    "tslint": "6.1.0",
    "tslint-angular": "3.0.3",
    "typescript": "4.2.4"
  }
}
